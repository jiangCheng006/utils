{
  "name": "@lishi/utils",
  "version": "1.0.1",
  "scripts": {
    "start": "vite --config vite.dev.config.ts --open",
    "build": "vue-tsc --noEmit && vite build --config vite.build.config.ts",
    "demo": "vue-tsc --noEmit && vite build --config vite.demo.config.ts",
    "build-all": "npm run build && npm run demo"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "main": "lib/bundle.umd.js",
  "module": "lib/bundle.es.js",
  "files": [
    "lib",
    "docs-dist",
    "README.md"
  ],
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  }
}
